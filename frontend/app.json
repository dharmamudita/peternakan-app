{
  "expo": {
    "name": "Peternakan App",
    "slug": "peternakan-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#D4A574"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.peternakan.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#D4A574"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.peternakan.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "platforms": [
      "ios",
      "android",
      "web"
    ]
  }
}